    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "nodemailer": "^6.9.7",
    "multer": "^1.4.5-lts.1"
